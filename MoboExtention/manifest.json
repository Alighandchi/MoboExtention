{
  "manifest_version": 3,
  "name": "MoboVPN",
  "version": "2.0",
  "description": "Powerful proxy management for Chrome.",
  "icons": {
    "16": "config/icons/icon16.png",
    "48": "config/icons/icon48.png",
    "128": "config/icons/icon128.png"
  },
  "background": {
    "service_worker": "config/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "config/popup.html",
    "default_icon": {
      "16": "config/icons/icon16.png",
      "48": "config/icons/icon48.png"
    }
  },
  "options_page": "config/options.html",
"permissions": [
    "proxy",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "browsingData",
    "nativeMessaging"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
